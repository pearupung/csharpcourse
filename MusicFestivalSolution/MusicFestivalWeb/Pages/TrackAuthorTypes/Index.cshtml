@page
@model MusicFestivalWeb.Pages.TrackAuthorTypes.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TrackAuthorType[0].TrackAuthorTypeName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.TrackAuthorType) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TrackAuthorTypeName)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.TrackAuthorTypeId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.TrackAuthorTypeId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.TrackAuthorTypeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
