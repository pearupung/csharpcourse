@page
@model MusicFestivalWeb.Pages.TrackAuthors.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>TrackAuthor</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TrackAuthor.Author)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TrackAuthor.Author.EmailAddress)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TrackAuthor.Track)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TrackAuthor.Track.TrackName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TrackAuthor.TrackAuthorType)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TrackAuthor.TrackAuthorType.TrackAuthorTypeName)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.TrackAuthor.TrackAuthorId">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
